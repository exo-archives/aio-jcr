<?xml version="1.0" encoding="UTF-8"?>
<connector version="1.5" xmlns="http://java.sun.com/xml/ns/j2ee"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/connector_1_5.xsd">
 <description>JCR RMI repository</description>
 <display-name>JCR RMI repository</display-name>
 <vendor-name>exoplatform</vendor-name>
 <eis-type/>
 <resourceadapter-version>1.0</resourceadapter-version>
 <license>
  <license-required>false</license-required>
 </license>
 <resourceadapter>
  <resourceadapter-class>org.exoplatform.connectors.jcr.adapters.rmi.JcrRmiResourceAdapter</resourceadapter-class>
  <config-property>
   <description/>
   <config-property-name>bindName</config-property-name>
   <config-property-type>java.lang.String</config-property-type>
   <config-property-value>rmirepository</config-property-value>
  </config-property>
  <config-property>
   <description/>
   <config-property-name>remoteName</config-property-name>
   <config-property-type>java.lang.String</config-property-type>
   <config-property-value>//localhost:9999/repository</config-property-value>
  </config-property>
 </resourceadapter>
</connector>
