<!--
	show some for all
-->
<propertyconfig>
	<nodetype>
		<name>*</name>
		<includes>
			<property>DAV:displayname</property>
			<property>DAV:resourcetype</property>
			<property>DAV:creationdate</property>
			<property>DAV:getcontentlength</property>
			<property>DAV:getlastmodified</property>			
		</includes>
		<excludes>
			<property>jcr:data</property>
			<property>jcr:versionHistory</property>
			<property>jcr:predecessors</property>
			<property>jcr:baseVersion</property>
			<property>exo:permissions</property>
		</excludes>
	</nodetype>
</propertyconfig>
	
